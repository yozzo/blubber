{
  "name": "blubber",
  "version": "0.0.1",
  "description": "Express JSON API Client CRUD framework",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ztolley/blubber.git"
  },
  "keywords": [
    "Express",
    "Crud",
    "JSON",
    "Framework"
  ],
  "author": "Zac Tolley <zac@thetolleys.com> (https://github.com/ztolley)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ztolley/blubber/issues"
  },
  "homepage": "https://github.com/ztolley/blubber#readme",
  "prettier": {
    "trailingComma": "es5",
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "jest"
    ],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/prefer-to-have-length": "warn",
      "jest/valid-expect": "error"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5"
  }
}
